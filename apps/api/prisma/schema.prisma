generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                  String          @unique @id
  email               String          @unique
  githubID            String          @unique
  name                String?
  mobile              String?

  bio                 String?
  avatar              String

  student             Boolean @default(true)
}